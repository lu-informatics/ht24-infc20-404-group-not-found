<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Pos?>

<VBox xmlns:fx="http://javafx.com/fxml" 
      fx:controller="se.lu.ics.controller.DeletedItemsController"
      alignment="CENTER" spacing="10">
    <Label text="Deleted Items" style="-fx-font-size: 24px; -fx-font-weight: bold;"/>
    <TableView fx:id="deletedItemsTable">
        <columns>
            <TableColumn fx:id="itemNameColumn" text="Item Name" />
        </columns>
    </TableView>
    <Button text="Close" onAction="#closeWindow"/>
</VBox>
